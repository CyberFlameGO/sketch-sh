{
  "name": "rtop",
  "type": "static",
  "public": true,
  "static": {
    "rewrites": [{ "source": "**", "destination": "/index.html" }],
    "headers": [
      {
        "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=311536000,public"
          }
        ]
      },
      {
        "source": "**.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=311536000,public"
          }
        ]
      },
      {
        "source": "index.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      },
      {
        "source": "embed.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }
}
